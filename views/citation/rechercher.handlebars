{{#if enseignants}}
<form action="#" method="POST">
	<div class="row valign-wrapper">
		<div class="col s12 m4 l4">
			<span>Enseignant : </span>
		</div>
		<div class="col s12 m8 l8">
			<select name="enseignant">
				<option value="-1">Tous</option>
				{{#each enseignants}}
				<option value="{{this.per_num}}">{{this.per_nom}}</option>
				{{/each}}
			</select>
		</div>
	</div>
	<div class="row valign-wrapper">
		<div class="col s12 m4 l4">
			<span>Date : </span>
		</div>
		<div class="col s12 m8 l8">
			<select name="date">
				<option value="-1" selected>Toutes</option>
				{{#each dates}}
				<option value="{{this.cit_date}}">{{this.cit_date}}</option>
				{{/each}}
			</select>
		</div>
	</div>
	<div class="row valign-wrapper">
		<div class="col s12 m4 l4">
			<span>Note : </span>
		</div>
		<div class="col s12 m8 l8">
			<select name="note">
				<option value="-1">Toutes</option>
				{{#each moyennes}}
				<option value="{{this.moyenne}}">{{this.moyenne}}</option>
				{{/each}}
			</select>
		</div>
	</div>
	<button type="submit" class="wave-effect wave-light btn">Rechercher</button>
</form>
{{else}}
	{{#if listeCitations}}
<table>
	<tr>
		<th>Enseignant</th>
		<th>Libellé</th>
		<th>Date</th>
		<th>Note</th>
	</tr>
		{{#each listeCitations}}
	<tr>
		<td>{{this.per_nom}}</td>
		<td>{{this.cit_libelle}}</td>
		<td>{{this.cit_date}}</td>
		<td>{{this.moyenne}}</td>
	</tr>
		{{/each}}
</table>
	{{else}}
	Pas de résultats :(
	{{/if}}
{{/if}}
